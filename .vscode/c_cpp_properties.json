{
    "configurations": [
        {
            "name": "Win32",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE"
            ],
            "configurationProvider": "ms-vscode.makefile-tools",
            "browse": {
                "limitSymbolsToIncludedHeaders": true
            },
            "mergeConfigurations": true,
            "compilerPath": "C:/msys64",
            "cppStandard": "c++98",
            "cStandard": "c11",
            "intelliSenseMode": "windows-clang-x64"
        }
    ],
    "version": 4
}